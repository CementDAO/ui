{
    "source": "./src",
    "destination": "./docs",
    "includes": [
        "\\.jsx$",
        "\\.tsx$"
    ],
    "excludes": [
        "\\.js$"
    ],
    "plugins": [
        {
            "name": "esdoc-standard-plugin"
        },
        {
            "name": "esdoc-react-plugin"
        },
        {
            "name": "esdoc-jsx-plugin"
        },
        {
            "name": "esdoc-typescript-plugin"
        }
    ]
}